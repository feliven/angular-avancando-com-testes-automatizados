{
  "name": "ng-test1",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-cf": "npx ng test --browsers Chrome,Firefox",
    "test-safari": "npx ng test --browsers Safari",
    "test-headless": "npx ng test --watch=false --browsers ChromeHeadless",
    "test-ffhl": "npx ng test --watch=false --browsers FirefoxHeadless",
    "test-ffhl2": "npx ng test --watch=false --browsers FirefoxSemInterface",
    "test-ci": "npx ng test --watch=false --reporters junit --browsers ChromeHeadless",
    "test-coverage": "npx ng test --watch=false --source-map=true --code-coverage=true --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "node ./server/server.js",
    "all": "concurrently \"npm run api\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.17",
    "@angular/common": "^19.2.17",
    "@angular/compiler": "^19.2.17",
    "@angular/core": "^19.2.17",
    "@angular/forms": "^19.2.17",
    "@angular/platform-browser": "^19.2.17",
    "@angular/platform-browser-dynamic": "^19.2.17",
    "@angular/router": "^19.2.17",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@types/node": "^24.10.1",
    "datauri": "^4.1.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.17",
    "@types/jasmine": "^5.1.13",
    "codelyzer": "^6.0.2",
    "concurrently": "^9.2.1",
    "jasmine-core": "^5.13.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^5.20.1",
    "typescript": "~5.8.3"
  }
}
